{
  "name": "viewer",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "npm run build --prefix=builders",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "predeploy": "ng run viewer:generate-aoc-metadata && ng build && npx shx cp dist/viewer/index.html dist/viewer/404.html",
    "deploy": "gh-pages -d dist/viewer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
